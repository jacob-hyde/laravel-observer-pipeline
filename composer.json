{
  "name": "jacobhyde/laravel-observer-pipeline",
  "description": "Explicit, ordered, and testable pipelines for Laravel Eloquent model events.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "eloquent",
    "observer",
    "pipeline",
    "attributes",
    "events"
  ],
  "authors": [
    {
      "name": "Jacob Hyde"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3|^8.4",
    "illuminate/contracts": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "JacobHyde\\ObserverPipeline\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JacobHyde\\ObserverPipeline\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JacobHyde\\ObserverPipeline\\ObserverPipelineServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
